<div id="wrapper">
        <!-- Sidebar -->
  <div id="sidebar-wrapper">
      <ul class="sidebar-nav">
          <li class="sidebar-brand">
              <a href="#">What would you like see?</a>
          </li>
          <li>
              <a class="glyphicon glyphicon-paperclip" aria-hidden="true" href="#"> Overview</a>
          </li>

          <li>
              <a class="glyphicon glyphicon-phone" aria-hidden="true" href="#"> Daily</a>
          </li>
          <li>
              <a class="glyphicon glyphicon-tasks" aria-hidden="true" href="#"> Category</a>
          </li>
          <li>
              <a class="glyphicon glyphicon-globe" aria-hidden="true" href="#"> Location</a>
          </li>
          <li>
              <a class="glyphicon glyphicon-picture" aria-hidden="true" href="#"> Trip</a>
          </li>
      </ul>
  </div>
  <!-- /#sidebar-wrapper -->

  <!-- Page Content -->
  <div id="page-content-wrapper">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12">
              <!-- <div id="sidebar"></div>
              <div> -->
              <p id="notice"><%= notice %></p>

                </div>
                  <a href="#menu-toggle" class="btn btn-default" id="menu-toggle">Menu</a>
                </div>

              <h1>Expense Details</h1>

              <table id='#test_div'>
                <thead>
                  <tr>
                    <th>Message</th>
                    <th>Cost</th>
                    <th>Category</th>
                    <th>Date</th>
                    <th>Location</th>
                    <th>User</th>
                    <th colspan="3"></th>
                  </tr>
                </thead >
                <tbody>
                  <% @expenses.each do |expense| %>
                    <tr>
                      <td><%= expense.textmsg %></td>
                      <td><%= expense.cost %></td>
                      <td><%= expense.category %></td>
                      <td><%= expense.date %></td>
                      <td><%= expense.location %></td>

                      <td><%= link_to 'Show', expense %></td>
                      <td><%= link_to 'Edit', edit_expense_path(expense) %></td>
                      <td><%= link_to 'Destroy', expense, method: :delete, data: { confirm: 'Are you sure?' } %></td>
                    </tr>
                  <% end %>
                </tbody>
              </table>

              <%= link_to 'New Expense', new_expense_path %>

          </div>
        </div>
      </div>
    </div>
  </div>


      <!-- Menu Toggle Script -->
    <script>
    $("#menu-toggle").click(function(e) {
        e.preventDefault();
        $("#wrapper").toggleClass("toggled");
    });
    </script>